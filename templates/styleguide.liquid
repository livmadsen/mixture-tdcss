{% layout "tdcss" %}

{% block content %}

<!-- # Buttons -->

<!-- : Button, standard -->
<button class="button">Button text</button>

<!-- : Button, strong -->
<button class="button--strong">Button text</button>

<!-- : Button, focus -->
<button class="button--focus">Button text</button>

<!-- : Button, small -->
<button class="button--small">Button text</button>

<!-- : Button, huge -->
<button class="button--huge">Button text</button>


<!-- : Button, combined, focus and huge -->
<button class="button--focus button--huge">Button text</button>



<!-- # Type styles -->
<!-- & Lists type setting with special font. Full font stack included â€“ remember to check your load flow. -->

<!-- : Stylised types, class -->

<div>
